{
  "manifest_version": 2,

  "name": "Bookimg",
  "description": "This extension allows users to create a Bookmark with a Screenshot",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Screenshot Bookmark"
  },
  "background": {
  	"scripts": ["background.js"]
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
